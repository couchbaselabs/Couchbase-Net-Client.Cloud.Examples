@model IEnumerable<TestApp.Models.Airline>
@{
    ViewData["Title"] = "Airlines";
}

<div class="row">
    <div class="col-md-3">
        <h2>Airlines</h2>
        <ul>
            @foreach (var airline in Model)
            {
                <li>@airline.Name</li>
            }
        </ul>
    </div>
    <div class="col-md-3">
    </div>
    <div class="col-md-3">
    </div>
    <div class="col-md-3">
    </div>
</div>
